<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.pittle.speedtest</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>./export.sh &gt;&gt; ./output.csv</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StartInterval</key>
	<integer>3600</integer>
	<key>UserName</key>
	<string>exalted</string>
	<key>GroupName</key>
	<string>staff</string>
	<key>WorkingDirectory</key>
	<string>/Users/exalted/Development/speedtest</string>
	<key>StandardOutPath</key>
	<string>/Users/exalted/Development/speedtest/debug.log</string>
	<key>StandardErrorPath</key>
	<string>/Users/exalted/Development/speedtest/debug.log</string>
</dict>
</plist>
